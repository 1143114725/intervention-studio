<?xml version="1.0" encoding="utf-8"?>
<selector
	xmlns:android="http://schemas.android.com/apk/res/android">
	<!-- camera_light  camera_dark -->
	<item
		android:state_pressed="true"
		android:drawable="@drawable/camera_dark" /> <!-- pressed -->
	<item
		android:state_focused="true"
		android:drawable="@drawable/camera_light" /> <!-- focused -->
	<item
		android:drawable="@drawable/camera_light" /> <!-- default -->
</selector>